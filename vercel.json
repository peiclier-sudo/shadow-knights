{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/js/$1",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      }
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1",
      "headers": {
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}